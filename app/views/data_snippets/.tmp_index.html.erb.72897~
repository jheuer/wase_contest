<table>
  <tr>
    <th>Body</th>
  </tr>

<% @data_snippets.each do |data_snippet| %>
  <tr>
    <td><%=h data_snippet.body %></td>
    <td><%= link_to 'Show', data_snippet %></td>
    <td><%= link_to 'Edit', edit_data_snippet_path(data_snippet) %></td>
    <td><%= link_to 'Destroy', data_snippet, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

